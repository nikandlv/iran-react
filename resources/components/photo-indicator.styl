.photo-indicator
    display flex
    align-items: center
    color white
    width 32px
    height 32px
    overflow hidden
    white-space nowrap
    box-sizing border-box
    border 2px solid white
    padding 0 10px
    border-radius 100px
    transition width 500ms
    &:hover
        width fit-content
    .title
        margin 0 30px